@using Pundit.Core.Server.Model
@model IEnumerable<DbPackage>
@{
    ViewBag.Title = "Packages";
}

<h2>There are @ViewBag.Count package(s)</h2>

<table>
   @foreach (DbPackage p in Model)
   {
      <tr>
         <td>@p.Package.PackageId @p.Package.VersionString (@p.Package.Platform)</td>
      </tr>
      <tr>
         <td>
            created: @p.CreatedDate, size: @p.FileSize   
         </td>
      </tr>
   }
</table>